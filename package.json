{
  "name": "ember-notify",
  "version": "1.2.0",
  "description": "Displays wee little notification messages down the bottom of your Ember.js app.",
  "main": "dist/cjs/main.js",
  "ember-addon": {
    "main": "ember-addon.js"
  },
  "scripts": {
    "test": "bower install && rm -rf dist && broccoli build dist && karma start --browsers PhantomJS --single-run",
    "build": "rm -rf dist && broccoli build dist"
  },
  "authors": [
    "Simon Wade"
  ],
  "repository": "https://github.com/aexmachina/ember-notify.git",
  "keywords": [
    "ember-addon"
  ],
  "files": [
    "dist/",
    "lib/",
    "test/",
    "ember-addon.js",
    "app-addon"
  ],
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.2.8",
    "broccoli": "^0.13.2",
    "broccoli-cli": "0.0.1",
    "broccoli-dist-es6-module": "^0.2.0",
    "broccoli-export-tree": "^0.3.2",
    "broccoli-merge-trees": "^0.2.1",
    "ember-cli-htmlbars": "^0.5.4",
    "karma": "~0.10.9",
    "karma-phantomjs-launcher": "~0.1.2",
    "karma-qunit": "~0.1.1"
  },
  "dependencies": {
    "broccoli-static-compiler": "^0.2.1"
  }
}
